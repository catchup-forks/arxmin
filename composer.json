{
    "name": "arx/arxmin",
    "type" : "Admin Management System",
    "description": "Arxmin is an universal admin management system for an Arx project",
    "homepage": "http://www.arx.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sum",
            "email": "daniel@cherrylabs.net",
            "homepage": "http://www.cherrylabs.net",
            "role": "Developer"
        }
    ],
    "require": {
        "arx/core" : "4.2.*",
        "zizaco/confide": "*",
        "zizaco/entrust": "1.2.*",
        "chumper/datatable" : "2.*",
        "fzaninotto/faker" : "*",
        "anahkiasen/former" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": {
            "Arxmin": "src"
        },
        "psr-4": {
            "Arxmin\\": "src/Arxmin"
        },
        "classmap": [
            "src/Arxmin/commands",
            "src/Arxmin/controllers",
            "src/Arxmin/models",
            "src/Arxmin/interfaces",
            "src/Arxmin/providers",
            "seeds",
            "migrations",
            "tests"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}