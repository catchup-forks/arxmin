{
  "name": "arx/arxmin",
  "type": "Admin Management System",
  "description": "Arxmin is an universal admin management system for an Arx project",
  "homepage": "http://www.arx.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Sum",
      "email": "daniel@cherrylabs.net",
      "homepage": "http://www.cherrylabs.net",
      "role": "Developer"
    }
  ],
  "require": {
    "arx/core": "5.*.*",
    "chumper/datatable": "2.*",
    "fzaninotto/faker": "*",
    "anahkiasen/former": "*",
    "barryvdh/laravel-ide-helper": "~2.0",
    "filp/whoops": "~1.0",
    "barryvdh/laravel-debugbar": "~2.0",
    "anahkiasen/former": "4.0.*@dev",
    "barryvdh/laravel-elfinder": "0.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "mockery/mockery": "0.7.2"
  },
  "autoload": {
    "psr-4": {
      "Arxmin\\": "src/"
    },
    "classmap": [
      "src/Arxmin/commands",
      "src/Arxmin/controllers",
      "src/Arxmin/models",
      "src/Arxmin/interfaces",
      "src/Arxmin/providers",
      "database/seeds",
      "database/migrations",
      "tests"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}